function : float nine_helper(float &x, float y, float &z) {
    x = 5.0;
    y = 5.0;
    z = 5.0;
    return x + y + z;
}

function : float nine(float x, float y, float z) {
    x = 10.5;
    cout << "x: " << x << ", y: " << y << ", z: " << z << endl;
    float fnh = nine_helper(x, y, z);
    cout << "x: " << x << ", y: " << y << ", z: " << z << endl;
    cout << fnh << endl;
    return fnh;
}

